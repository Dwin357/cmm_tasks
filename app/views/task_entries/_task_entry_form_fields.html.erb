<div class='form-group'>
  <%= f.label :init_time %>
  <%= f.time_field :init_time %>
</div>
<div class='form-group'>
  <%= f.label :init_date %>
  <%= f.date_field :init_date %>
</div>
<div class='form-group'>
  <%= f.label :completion_time %>
  <%= f.text_field :completion_time, placeholder: "min" %>
</div>
<div class='form-group'>
  <%= f.label :note %>
  <%= f.text_area :note %>
</div>
<div class='form-group'>
  <%= f.submit %>
</div>

