<div class='row'>
  <div class='col-xs-10 col-xs-offset-1'>
    <div class='well'>
      <p>
        <%= @task_entry.start_time.to_formatted_s(:long_ordinal)%>
        <strong> to </strong>
        <%= @task_entry.end_time.to_formatted_s(:long_ordinal) %>
      </p>
    </div>
    <p>
      <%= link_to "<< Task", task_path(@task_entry.task) %> - 
      <%= link_to "Edit Entry", edit_task_entry_path(@task_entry) %>
    </p>
    <p><%= @task_entry.note %></p>
    
  </div>
</div>